const asnc=(sayi)=>{
    return new Promise((resolve,reject)=> {
    if(sayi===4)
        resolve('Her şey tıkırında!');
    else 
        reject('Bir sorun var');
    })
};
asnc(4)
    .then((data) => {
        console.log(data);
        return 1;
    })
    .then((data) => {
        console.log(sayi);
        return data+6;
    })
    .then((data) => {
        console.log(data);
    })
    .catch((error) => {
        console.log(error);
    })
